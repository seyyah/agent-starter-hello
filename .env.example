# OpenServ API key (required for platform integration)
OPENSERV_API_KEY=your_openserv_api_key_here

# OpenAI API key (optional, for local testing)
OPENAI_API_KEY=your_openai_api_key_here

# Port for the agent's server (default: 7378)
PORT=7378

# Application settings
NODE_ENV=development
LOG_LEVEL=debug
MAX_RANGE_SIZE=1000
